{
  "version": 2,
  "routes": [
    {
      "src": "/admin/?$",
      "dest": "/admin/login.html"
    },
    {
      "src": "/admin/login.html$",
      "dest": "/admin/login.html"
    },
    {
      "src": "/admin/index.html$",
      "dest": "/admin/index.html"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/admin/$1"
    }
  ]
}